{
  "name": "snake-coin-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "gulp serve",
    "lint": "esw *.js src --color",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --timeout 5000 --reporter spec --colors --require babel-polyfill --require babel-core/register src/tests --recursive",
    "test:watch": "yarn test --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --require babel-polyfill --require babel-core/register src/tests --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "~1.18.2",
    "chai": "^4.1.2",
    "compression": "^1.7.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^5.0.0",
    "express": "~4.15.5",
    "express-swagger-generator": "^1.0.4",
    "helmet": "^3.11.0",
    "http-status": "^1.0.1",
    "method-override": "^2.3.10",
    "mongoose": "^5.0.5",
    "morgan": "~1.9.0",
    "sha256": "^0.2.0",
    "supertest": "^3.0.0",
    "swagger-express": "^1.0.5",
    "swagger-jsdoc": "^1.9.7",
    "swagger-ui-express": "^2.0.15"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.3",
    "del": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.4.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.8",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^5.0.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "run-sequence": "^2.2.1"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
